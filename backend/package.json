    "seed": "yarn ts-node-custom ./src/infra/database/seed.ts",
    "seed2": "yarn ts-node-custom ./src/infra/database/seed2.ts",
    "db:setup": "chmod +x database.sh && ./database.sh",
    "db:start": "./database.sh start",
    "db:stop": "./database.sh stop",
    "db:restart": "./database.sh restart",
    "db:logs": "./database.sh logs",
    "db:psql": "./database.sh psql",
    "db:seed": "yarn seed",
    "db:fresh": "npx prisma migrate reset --force && yarn seed",
    "setup": "chmod +x setup-env.sh && ./setup-env.sh",
    "full-setup": "yarn setup && yarn db:setup && yarn install && npx prisma generate && npx prisma migrate dev && yarn seed"
